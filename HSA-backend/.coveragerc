[report]
exclude_also =
    def __repr__
    def __str__
    if self.debug:
    if settings.DEBUG

omit =
  */__init__.py 
  hsabackend/management/*
  hsabackend/middleware/*
  hsabackend/migrations/*
  hsabackend/asgi.py
  hsabackend/models.py  
  hsabackend/urls.py
  hsabackend/views/index.py
  hsabackend/wsgi.py
  hsabackend/settings.py
  hsabackend/tests/*
  hsabackend/serializers/*
  manage.py
  features/environment.py
  hsabackend/signalhandlers/*
  hsabackend/utils/env_utils.py # used to get backend url
  hsabackend/utils/password_reset_route_adder.py # used to add rate limiting, this is code ripped from the library